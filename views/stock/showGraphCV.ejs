<%- include ../header %>
<script>
  APP.data = <%- JSON.stringify(params.data) %>;
	seajs.use("js_cmd/stock/showGraphCV-cmd");
</script>
<link href="/css/stock/showGraphCV.css?v=<%= config.version %>" rel="stylesheet" />
<nav class="navbar navbar-default" role="navigation">
	<div class="navbar-header">
		<a class="navbar-brand" href="#">SP</a>
	</div>
	<div>
		<ul class="nav navbar-nav navbar-right mr ml">
			<li>
				<p class="navbar-text">欢迎您，<%= params.user.username %></p>
			</li>
			<li>
				<a href="/user/logout">退出</a>
			</li>
		</ul>
	</div>
</nav>
<div class="container">
	<div class="row">
		<div class="col-sm-2">
			<div class="panel panel-default">
				<div class="panel-heading">
					<h3 class="panel-title">个股查询</h3>
				</div>
				<div class="panel-body">
					<div class="form-group">
						<label for="market">选择股市</label>
						<select id="market" class="form-control">
							<option value="sh">上证</option>
							<option value="sz">深证</option>
						</select>
					</div>
					<div class="form-group">
						<label for="symbol">股票代码</label>
						<input id="symbol" class="form-control" value="000001">
					</div>
					<div class="form-group">
						<button type="button" id="btn_lookup_one" class="btn btn-default">查看</button>
					</div>
				</div>
			</div>
			<div class="panel panel-default">
				<div class="panel-heading">
					<h3 class="panel-title">板块查询</h3>
				</div>
				<div class="panel-body">
					<div class="form-group">
						<label for="stock_block">选择板块</label>
						<select id="stock_block" class="form-control">
							<option value selected="selected" disabled="disabled">请选择板块</option>
							<option value="1">sh000001</option>
						</select>
					</div>
					<div class="form-group">
						<button type="button" id="btn_lookup_block" class="btn btn-default">查看</button>
						<a href="/block/editBlock" class="btn btn-primary">编辑</a>
					</div>
				</div>
			</div>
		</div>
		<div class="col-sm-10">
			<div id="stock_graph"></div>
		</div>
	</div> 
</div>


<%- include ../footer %>